\n45\t    public FluidStack getFluid() {\n46\t        return container.getOrDefault(componentType, SimpleFluidContent.EMPTY).copy();\n47\t    }\n48\t\n49\t    protected void setFluid(FluidStack fluid) {\n50\t        container.set(componentType, SimpleFluidContent.copyOf(fluid));\n51\t    }\n52\t\n53\t    @Override\n54\t    public int getTanks() {\n55\t        return 1;\n56\t    }\n57\t\n58\t    @Override\n59\t    public FluidStack getFluidInTank(int tank) {\n60\t        return getFluid();\n61\t    }\n62\t\n63\t    @Override\n64\t    public int getTankCapacity(int tank) {\n65\t        return capacity;\n66\t    }\n67\t\n68\t    @Override\n69\t    public boolean isFluidValid(int tank, FluidStack stack) {\n70\t        return true;\n71\t    }\n72\t\n73\t    @Override\n74\t    public int fill(FluidStack resource, FluidAction doFill) {\n75\t        if (container.getCount() != 1 || resource.isEmpty() || !canFillFluidType(resource)) {\n76\t            return 0;\n77\t        }\n78\t\n79\t        FluidStack contained = getFluid();\n80\t        if (contained.isEmpty()) {\n81\t            int fillAmount = Math.min(capacity, resource.getAmount());\n82\t\n83\t            if (doFill.execute()) {\n84\t                setFluid(resource.copyWithAmount(fillAmount));\n85\t            }\n86\t\n87\t            return fillAmount;\n88\t        } else {\n89\t            if (FluidStack.isSameFluidSameComponents(contained, resource)) {\n90\t                int fillAmount = Math.min(capacity - contained.getAmount(), resource.getAmount());\n91\t\n92\t                if (doFill.execute() && fillAmount > 0) {\n93\t                    contained.grow(fillAmount);\n94\t                    setFluid(contained);\n95\t                }\n96\t\n97\t                return fillAmount;\n98\t            }\n99\t\n100\t            return 0;\n101\t        }\n102\t    }\n103\t\n104\t    @Override\n105\t    public FluidStack drain(FluidStack resource, FluidAction action) {\n106\t        if (container.getCount() != 1 || resource.isEmpty() || !FluidStack.isSameFluidSameComponents(resource, getFluid())) {\n107\t            return FluidStack.EMPTY;\n108\t        }\n109\t        return drain(resource.getAmount(), action);\n110\t    }\n111\t\n112\t    @Override\n113\t    public FluidStack drain(int maxDrain, FluidAction action) {\n114\t        if (container.getCount() != 1 || maxDrain <= 0) {\n115\t            return FluidStack.EMPTY;\n116\t        }\n117\t\n118\t        FluidStack contained = getFluid();\n119\t        if (contained.isEmpty() || !canDrainFluidType(contained)) {\n120\t            return FluidStack.EMPTY;\n121\t        }\n122\t\n123\t        final int drainAmount = Math.min(contained.getAmount(), maxDrain);\n124\t\n125\t        FluidStack drained = contained.copyWithAmount(drainAmount);\n126\t\n127\t        if (action.execute()) {\n128\t            contained.shrink(drainAmount);\n129\t            if (contained.isEmpty()) {\n130\t                setContainerToEmpty();\n131\t            } else {\n132\t                setFluid(contained);\n133\t            }\n134\t        }\n135\t\n136\t        return drained;\n137\t    }\n138\t\n139\t    public boolean canFillFluidType(FluidStack fluid) {\n140\t        return true;\n141\t    }\n142\t\n143\t    public boolean canDrainFluidType(FluidStack fluid) {\n144\t        return true;\n145\t    }\n146\t\n147\t    /**\n148\t     * Override this method for special handling.\n149\t     * Can be used to swap out or destroy the container.\n150\t     */\n151\t    protected void setContainerToEmpty() {\n152\t        container.remove(componentType);\n153\t    }\n154\t\n155\t    /**\n156\t     * Destroys the container item when it's emptied.\n157\t     */\n158\t    public static class Consumable extends FluidHandlerItemStack {\n159\t        public Consumable(Supplier<DataComponentType<SimpleFluidContent>> componentType, ItemStack container, int capacity) {\n160\t            super(componentType, container, capacity);\n161\t        }\n162\t\n163\t        @Override\n164\t        protected void setContainerToEmpty() {\n165\t            super.setContainerToEmpty();\n166\t            container.shrink(1);\n167\t        }\n168\t    }\n169\t\n170\t    /**\n171\t     * Swaps the container item for a different one when it's emptied.\n172\t     */\n173\t    public static class SwapEmpty extends FluidHandlerItemStack {\n174\t        protected final ItemStack emptyContainer;\n175\t\n176\t        public SwapEmpty(Supplier<DataComponentType<SimpleFluidContent>> componentType, ItemStack container, ItemStack emptyContainer, int capacity) {\n177\t            super(componentType, container, capacity);\n178\t            this.emptyContainer = emptyContainer;\n179\t        }\n180\t\n181\t        @Override\n182\t        protected void setContainerToEmpty() {\n183\t            super.setContainerToEmpty();\n184\t            container = emptyContainer;\n185\t        }\n186\t    }\n187\t}\n188"
